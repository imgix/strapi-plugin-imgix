{
  "name": "strapi-plugin-imgix",
  "version": "1.0.3",
  "description": "Strapi - imgix Integration Plugin",
  "strapi": {
    "name": "imgix",
    "description": "Unleash the Potential of Your Media with AI | An imgix integration for any kind of Strapi Media Library provider",
    "kind": "plugin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imgix/strapi-plugin-imgix"
  },
  "dependencies": {
    "@strapi/design-system": "^1.18.0",
    "@strapi/helper-plugin": "^4.25.12",
    "@strapi/icons": "^1.18.0",
    "@tanstack/react-query": "^5.40.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@strapi/strapi": "^4.25.12",
    "@strapi/types": "^4.25.12",
    "@strapi/typescript-utils": "^4.25.12",
    "@types/jest": "^29.5.12",
    "@types/koa": "^2.15.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/lodash": "^4.17.4",
    "@types/react": "^17.0.53",
    "@types/react-dom": "^18.0.28",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.32",
    "husky": "7.0.4",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-styled-components": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.3.4",
    "styled-components": "^5.3.6",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.1",
    "typescript": "5.3.2"
  },
  "peerDependencies": {
    "@strapi/strapi": "^4.0.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1"
  },
  "author": {
    "name": "imgix",
    "email": "sdk@imgix.com",
    "url": "https://imgix.com"
  },
  "engines": {
    "node": ">=18.0.0 <=20.x.x",
    "npm": ">=6.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && yarn test:unit"
    }
  },
  "nodemonConfig": {
    "ignore": [
      "./dist/**/*",
      "./__mocks__/*",
      "./**/__tests__/*"
    ],
    "ext": "js,json,ts"
  },
  "scripts": {
    "prepare": "husky install",
    "publish:latest": "cd dist && npm publish --tag latest",
    "prepublish:latest": "npm run clean && npm run build && node dist/setup-package.js",
    "build": "tsc",
    "build:dev": "yarn run build && cp ./package.json ./dist",
    "clean": "rm -rf dist",
    "develop": "nodemon --exec \"yarn build:dev\"",
    "test:unit": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:ci": "CI=true jest --ci --runInBand --verbose --coverage",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "keywords": [
    "strapi",
    "plugin",
    "imgix",
    "upload"
  ],
  "license": "MIT"
}
